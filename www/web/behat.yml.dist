default:
    suites:
        login:
            contexts:
                - Api\LoginContext

        register:
            paths:
                - "%paths.base%/features/register"
            contexts:
                - Api\RegisterContext

    extensions:
        FriendsOfBehat\SymfonyExtension: ~
        Behat\MinkExtension:
            sessions:
                symfony:
                    symfony: ~

